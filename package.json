{
  "name": "proj-starter",
  "version": "0.0.0",
  "repository": "https://github.com/Chigozie391/proj-starter",
  "license": "MIT",
  "main": "dist/server.js",
  "keywords": [],
  "scripts": {
    "start": "NODE_ENV=production npm run deploy && node dist/server.js",
    "deploy": "npm run build && npm run migrate",
    "migrate": "sequelize db:migrate",
    "start:dev": "ts-node-dev --files --respawn ./src/server.ts",
    "test": "jest --forceExit --setupFiles dotenv/config --detectOpenHandles --coverage",
    "test:watch": "jest --setupFiles dotenv/config --watch --detectOpenHandles",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "build": "tsc -p ./"
  },
  "engines": {
    "node": "12.16.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "celebrate": "^15.0.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "hpp": "^0.2.3",
    "joi": "^17.4.2",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "pg": "^8.7.1",
    "sequelize-cli": "^6.2.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.5.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/hpp": "^0.2.1",
    "@types/jest": "^26.0.20",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.31",
    "@types/supertest": "^2.0.10",
    "jest": "^26.6.3",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.1",
    "ts-node-dev": "^1.1.8",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^4.1.5"
  }
}
